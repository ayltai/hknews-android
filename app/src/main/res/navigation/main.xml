        android:name="com.github.ayltai.hknews.app.ListFragment$Cozy"
        android:label="action_news_cozy"
        tools:layout="@layout/fragment_list">
        <action
            android:id="@+id/detailsAction"
            app:destination="@+id/detailsFragment"
